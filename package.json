{
  "name": "react-babel-7",
  "version": "1.0.0",
  "description": "react boiler plate with babel 7",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --port 8080",
    "build": "rm -rf dist && TARGET_ENV=stage NODE_ENV=production webpack --mode production",
    "sync-stage-js": "aws s3 sync dist/js s3://alias-manager-stage/js --cache-control no-cache --content-encoding gzip",
    "sync-stage": "aws s3 sync dist/ s3://alias-manager-stage/ --cache-control no-cache --exclude js/*",
    "deploy-stage": "npm run build && npm run sync-stage && npm run sync-stage-js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "babel",
    "7",
    "react"
  ],
  "author": "hepiska",
  "license": "ISC",
